{
  "redirects": [
    {
      "source": "/pages/categorias/:path.html",
      "destination": "/categorias/:path",
      "permanent": true
    },
    {
      "source": "/pages/otros/:path.html",
      "destination": "/:path",
      "permanent": true
    },
    {
      "source": "/index.html",
      "destination": "/",
      "permanent": true
    }
  ],

  "rewrites": [
    {
      "source": "/categorias/:path",
      "destination": "/pages/categorias/:path.html"
    },
    {
      "source": "/:path",
      "destination": "/pages/otros/:path.html"
    },
    {
      "source": "/",
      "destination": "/index.html"
    }
  ]
}
